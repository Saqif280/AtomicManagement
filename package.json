{
  "name": "atomic-management",
  "main": "./lib/atomic-management",
  "version": "0.1.2",
  "description": "Per-project Atom text editor configurations, for each window",
  "keywords": [],
  "activationCommands": {},
  "repository": "https://github.com/saqif280/atomic-management",
  "license": "MIT",
  "engines": {
    "atom": ">=1.26.0 <2.0.0"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "dependencies": {
    "font-awesome": "^4.7.0",
    "glob": "^7.1.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "season": "^6.0.2"
  },
  "configSchema": {
    "isEnabled": {
      "description": "Enable loading project configurations",
      "type": "boolean",
      "default": true
    },
    "enforceConfiguredPackages": {
      "description": "Alert the user when configured packages in project configurations are not installed",
      "type": "boolean",
      "default": true
    }
  }
}
